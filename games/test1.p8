--bonnus

function cree_bonus()
	bonnu = {
	x=rnd(112),
	y=-8,
	speed=1
	}
	if p.life<4 
	   and #bonus==0
	   and p.score > 300 then
 	add(bonus,bonnu)
 end
end

function update_bonus()
	for b in all(bonus) do
		b.y+=b.speed
		if colition(b,p) then
 		del(bonus,b)
 		sfx(4)
 		p.life+=1
 	end
 	if b.y>128 then
 	 del(bonus,b)
 	end
	end
end
