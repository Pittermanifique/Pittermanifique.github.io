--enemis

function add_enemi(niv)
 if niv == 1 then
		enemi = {
		niveu = 1,
		x=rnd(112),
		y=-8,
		speed=rnd(2)+1,
		life=2
		}
	elseif niv == 2 then
		enemi = {
		niveu = 2,
		x=rnd(112),
		y=-8,
		speed=rnd(1)+0,8,
		life=4
		}
	end
	add(enemis,enemi)
end

function update_enemis()
 if #enemis<p.dificulte*2 then
  if p.dificulte <= 3 then
 		add_enemi(1)
 	elseif p.dificulte > 3 then
 	 if rnd(100) > 60 then
 	  add_enemi(2)
 	 else
 	  add_enemi(1)
 	 end
 	end
 end
 for e in all(enemis) do
 	e.y+=e.speed
 	if e.y>128 then
 	 del(enemis,e)
 	end
 	if colition(e,p) then
 		del(enemis,e)
 		sfx(3)
 		p.life-=1
 	end
 	for b in all(bullets) do
 		if colition(e,b) then
 		 del(bullets,b)
 		 sfx(2)
 		 e.life-=1
 	if e.life == 0 then
 		del(enemis,e)
 		sfx(3)
 		p.score+=10
 	end
 		end
 	end
 end
end
