--bullets

function cree_bullet()
 speed = 4
 bullet = {x=p.x,y=p.y,s=speed}
 add(bullets,bullet)
end

function update_bullets()
	for i in all(bullets) do
		i.y-=i.s
		if i.y<-8 then
			del(bullets,i)
		end
	end
end
		
