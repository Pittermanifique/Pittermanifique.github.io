__lua__
function _init()
	p = {x=64,
	y=90,
	speed=1.5,
	life=3,
	score=0,
	etat=1,
	dificulte=1 
	}
	bullets = {}
	enemis = {}
	bonus = {}
end

function _update()
	if p.etat != 0 then
	 pmouvements()
	 dificulty()
	 update_bonus()	 
		update_bullets()
		update_enemis()
		verif_life()
		if flr(rnd(1000)) == 1 then
			cree_bonus()
		end
	else
		rungameover()
	end
end

function _draw()
	if p.etat != 0 then
		cls()
		map(0,0,0,0)
		for b in all(bullets) do
			spr(2,b.x,b.y)
		end
		for e in all(enemis) do
		 if e.niveu == 1 then
				spr(5,e.x,e.y)
			elseif e.niveu == 2 then
			 spr(6,e.x,e.y)
			end
		end
		for b in all(bonus) do
			spr(4,b.x,b.y)
		end
		spr(1,p.x,p.y)
		draw_ui()
	elseif p.etat == 0 then
		game_over()
	end
end

